{
  "name": "cordova-osx",
  "version": "6.0.0",
  "description": "cordova-osx release",
  "main": "bin/templates/scripts/cordova/Api.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/apache/cordova-osx"
  },
  "bugs": {
    "url": "https://github.com/apache/cordova-osx/issues"
  },
  "keywords": [
    "osx",
    "cordova",
    "apache",
    "ecosystem:cordova",
    "cordova:platform"
  ],
  "scripts": {
    "test": "npm run lint && npm run test:coverage",
    "test:objc": "jasmine  --config=tests/spec/objc.json",
    "test:component": "jasmine --config=tests/spec/component.json",
    "test:coverage": "nyc jasmine --config=tests/spec/coverage.json",
    "lint": "eslint . \"bin/**/!(*.*)\""
  },
  "author": "Apache Software Foundation",
  "license": "Apache-2.0",
  "dependencies": {
    "cordova-common": "^4.0.0",
    "nopt": "^4.0.1",
    "plist": "^3.0.1",
    "q": "^1.4.1",
    "shelljs": "^0.5.3",
    "underscore": "^1.9.1",
    "unorm": "^1.4.1",
    "xcode": "^3.0.1"
  },
  "devDependencies": {
    "@cordova/eslint-config": "^3.0.0",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "nyc": "^14.1.1",
    "tmp": "0.0.33"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "nyc": {
    "include": [
      "bin/lib/**",
      "bin/templates/scripts/**"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
